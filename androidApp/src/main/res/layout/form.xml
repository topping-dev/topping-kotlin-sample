<?xml version="1.0" encoding="UTF-8"?>
<LGLinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:lua="http://schemas.android.com/apk/res-auto"
	lua:id="formTestLL"
	android:id="@+id/formTestLL"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:background="#7993AB">
	<AutoCompleteTextView
	    android:id="@+id/formTestAutoCompTV"
	    android:layout_width="match_parent"
    	android:layout_height="wrap_content">
	</AutoCompleteTextView>
	<Button
	    android:id="@+id/formTestButton"
	    android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:text="Button Text">
	</Button>
	<CheckBox
		android:id="@+id/formTestCheckBox"
	    android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:text="Checkbox text"
        android:checked="@bool/checkbox_checked">
	</CheckBox>
	<EditText
		android:id="@+id/formTestEt"
	    android:layout_width="match_parent"
    	android:layout_height="wrap_content"
        android:hint="test hint">
	</EditText>
	<ProgressBar
		android:id="@+id/formTestProgressBar"
	    android:layout_width="match_parent"
    	android:layout_height="wrap_content"
        android:background="#ffffff"
        android:progress="15">
	</ProgressBar>
	<ImageView
		android:id="@+id/formTestIv"
	    android:layout_width="40dp"
    	android:layout_height="wrap_content"
        android:src="@drawable/indexbutleft">
	</ImageView>
</LGLinearLayout>
